{
  "name": "experience-template",
  "version": "0.0.1",
  "description": "a template for creating super-xr content",
  "main": "./src/index.js",
  "scripts": {
    "build:dev": "webpack --config ./webpack/webpack.config.js --watch=false --mode=development --devbuild=true",
    "build:prod": "webpack --config ./webpack/webpack.config.js --watch=false --mode=production",
    "http": "http-server ./dist -g -p 8080",
    "test": "nodemon",
    "dev": "webpack-dev-server --config webpack/webpack.config.js --open --mode=development",
    "purge": "purge.sh",
    "ngrok-serve": "ngrok http 8080 -subdomain=mandatoryvr",
    "start": "concurrently \"npm:ngrok-serve\" \"npm:http\""
  },
